alias a="alias"
alias h="history"
alias l="ls"
alias ll="ls -l"
alias lll="ls -a"
alias llll="ls -al"
alias llt="cds; ll target"
alias go="cds; target/sqlgo"
alias cdg="cd; cd github"
alias cds="cdg; cd sql_go"
alias cdc="cds; cd core"
alias cdt="cds; cd test_dir"
alias cdu="cds; cd utils_dir"
alias cdua="cdu; cd abend_dir"
alias cduq="cdu; cd queue_mgr_dir"
alias cdut="cdu; cd thread_mgr_dir"
alias makego="cds; make"
alias au="cds; cp tools/alias_phwang ~/.alias_phwang"
alias gitclonesql="cdg; git clone https://github.com/paulhwang/sql_go; cds; mkdir target; mkdir db"
